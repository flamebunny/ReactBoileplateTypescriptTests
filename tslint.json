{
  /*
   * Possible values:
   * - the name of a built-in config
   * - the name of an NPM module which has a "main" file that exports a config object
   * - a relative path to a JSON file
   */
  "extends": ["tslint:latest", "tslint-react", "tslint-eslint-rules"],
  "rules": {
    "indent": [
      true,
      "spaces",
      2
    ],
    "ter-indent": [
      true,
      2,
      {
        "SwitchCase": 1
      }
    ],
    "quotemark": [
      true,
      "jsx-single",
      "single"
    ],
    "semicolon": [
      true,
      "never"
    ],
    "trailing-comma": [
      true,
      "never"
    ],
    "max-classes-per-file": false,
    "max-line-length": [
      false
    ],
    "object-literal-sort-keys": false,
    "ordered-imports": [
      false
    ],
    "jsdoc-format": false,
    "no-var-requires": false,
    "whitespace": [
      true,
      "check-branch",
      "check-decl",
      "check-operator",
      "check-module",
      "check-separator",
      "check-type"
    ],
    "arrow-parens": [
      true,
      "ban-single-arg-parens"
    ],
    "interface-name": [
      true,
      "never-prefix"
    ],
    "object-literal-key-quotes": false,
    "no-empty": false,
    "no-empty-interface": false,
    "no-implicit-dependencies": false,
    "no-submodule-imports": false,
    "object-curly-spacing": [
      true,
      "always"
    ],
    "jsx-space-before-trailing-slash": [
      true
    ],
    "jsx-no-multiline-js": false,
    "jsx-no-lambda": false
  },
  "rulesDirectory": [
    /*
     * A list of relative or absolute paths to directories that contain custom rules.
     * See the Custom Rules documentation below for more details.
     */
  ]
}
