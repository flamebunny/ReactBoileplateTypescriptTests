    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4",
    "webpack-manifest-plugin": "1.3.2"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "./node_modules/.bin/jest --testMatch **/src/**/*.test.{ts,tsx} --coverage",
    "test2": "node scripts/test.js --env=jsdom"
  },
  "jest": {
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "setupFiles": [
      "<rootDir>/setup-jest.js"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": "src/tsconfig.build.json"
      }
    },
    "coverageReporters": [
      "html",
      "text-summary"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 70,
        "functions": 70,
        "lines": 80,
        "statements": 80
      }
    }
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
